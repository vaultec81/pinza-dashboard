{
  "name": "pinza-dashboard",
  "version": "0.0.1",
  "description": "React + Electron based interface for pinza",
  "main": "./dist/main.prod.js",
  "scripts": {
    "clear": "rm -rf ./dist",
    "dev": "npm run start-renderer",
    "prod": "run-electron .",
    "package": "npm run build && electron-packager . --overwrite --out=build",
    "build": "npm run clear && npm-run-all --parallel build-main build-renderer",
    "build-main": "webpack --config webpack/main.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js",
    "start-main": "cross-env NODE_ENV=development run-electron -r @babel/register src/main/index.js",
    "start-renderer": "cross-env NODE_ENV=development webpack-dev-server --port 6789 --config webpack/renderer.dev.config.js"
  },
  "keywords": [
    "electron",
    "react",
    "pinza"
  ],
  "author": "vaultec81",
  "license": "GPLv3",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "run-electron": "^0.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@trendmicro/react-sidenav": "^0.5.0",
    "bootstrap": "^4.5.0",
    "electron-packager": "^14.2.1",
    "electron-promise-ipc": "^2.1.0",
    "go-ipfs-dep": "^0.5.1",
    "ipfs-http-client": "^44.2.0",
    "orbit-db-identity-provider": "^0.3.0",
    "pinza": "0.1.0-rc.3",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.8.5",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0"
  }
}
